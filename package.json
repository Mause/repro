{
  "name": "@mause/repro",
  "version": "0.3.0",
  "description": "Simple tool to quickly run reproduction code from a GitHub issue",
  "author": "Elliana May @Mause",
  "bin": {
    "repro": "./bin/run"
  },
  "homepage": "https://github.com/Mause/repro",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/Mause/repro.git",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.16.4",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-warn-if-update-available": "^2.0.6",
    "@octokit/rest": "^19.0.4",
    "@textlint/markdown-to-ast": "^12.2.1",
    "inquirer": "^9.1.2",
    "cli-highlight": "^2.1.11",
    "prettier": "^2.7.1"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/inquirer": "^9.0.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.56",
    "chai": "^4",
    "eslint": "^8.0.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "globby": "^13.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "mocha": "^10.0.0",
    "oclif": "^3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "repro",
    "dirname": "repro",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Mause/repro/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
